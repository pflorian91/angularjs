  "bugs": {
    "url": "https://github.com/pflorian91/angularjs/issues"
  },
  "homepage": "https://github.com/pflorian91/angularjs#readme",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.3",
    "angular": "^1.6.4"
  },
  "devDependencies": {
    "@types/angular": "^1.6.17",
    "@types/angular-mocks": "^1.5.9",
    "@types/jasmine": "^2.5.47",
    "@types/webpack": "^2.2.15",
    "@types/webpack-env": "^1.13.0",
    "angular-mocks": "^1.6.4",
    "autoprefixer": "^7.1.1",
